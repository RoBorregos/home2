<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>pick_and_place</name>
  <version>0.0.0</version>
  <description>Source code of Frida robot pymoveit2 adapters and samples</description>
  <maintainer email="roborregos@gmail.com">ros</maintainer>
  <license>TODO: License declaration</license>

  <buildtool_depend>ament_cmake</buildtool_depend>
  <buildtool_depend>ament_cmake_python</buildtool_depend>

  <depend>rclcpp</depend>
  <depend>control_toolbox</depend>
  <depend>controller_manager</depend>
  <depend>xarm_description</depend>
  <depend>xarm_controller</depend>
  <depend>uf_ros_lib</depend>
  <depend>xarm_moveit_config</depend>
  <depend>frida_interfaces</depend>
  <depend>frida_constants</depend>
  <depend>object_detection_handler</depend>
  <depend>object_detector_2d</depend>
