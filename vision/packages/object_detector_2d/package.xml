<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>object_detector_2d</name>
  <version>0.0.0</version>
  <description>The object_detector_2d package for ROS 2</description>
  <maintainer email="roborregosteam@gmail.com">RoBorregos</maintainer>
  <license>TODO</license>

  <!-- Build tool dependencies -->
  <buildtool_depend>ament_cmake</buildtool_depend>
  <buildtool_depend>ament_cmake_python</buildtool_depend>
  <buildtool_depend>frida_interfaces</buildtool_depend>
  <buildtool_depend>frida_constants</buildtool_depend>

